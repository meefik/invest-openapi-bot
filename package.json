{
  "name": "kormobot",
  "version": "1.5.0",
  "description": "Tinkoff.Invest trading bot",
  "author": "Anton Skshidlevsky <meefik@gmail.com>",
  "main": "server.js",
  "dependencies": {
    "@tinkoff/invest-openapi-js-sdk": "^1.5.0",
    "express": "^4.18.1",
    "nconf": "^0.12.0"
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "standard",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ]
    }
  }
}